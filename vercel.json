{
  "rewrites": [
    {
      "source": "/dashboard/:path*",
      "destination": "/dashboard"
    },
    {
      "source": "/:path*",
      "destination": "/"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}